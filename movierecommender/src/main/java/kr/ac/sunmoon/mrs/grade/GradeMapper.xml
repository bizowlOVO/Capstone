<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.ac.sunmoon.mrs.grade.GradeMapper">	
	<select id="selectRankGrade" resultType="kr.ac.sunmoon.mrs.agent.Grade" >
		SELECT MOVIE_SEQ
		FROM GRADE
		GROUP BY MOVIE_SEQ
		ORDER BY COUNT(MOVIE_SEQ) DESC;
	</select>
</mapper>